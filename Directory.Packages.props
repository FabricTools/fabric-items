    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    <navidataUtilsVersion>1.1.0</navidataUtilsVersion>
    <FabricToolsCodeGenVersion>1.1.0</FabricToolsCodeGenVersion>
