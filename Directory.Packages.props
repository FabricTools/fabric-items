    <PackageVersion Include="navidataIO.Utils.Json" Version="$(navidataUtilsVersion)" />
    <PackageVersion Include="navidataIO.Utils.Testing.Xunit3" Version="$(navidataUtilsVersion)" />
  </ItemGroup>
  <ItemGroup>
    <GlobalPackageReference Include="DotNet.ReproducibleBuilds" Version="1.2.25" />
